<h1><%=@airline.name%> Pilot Records</h1>

<% @airline.pilots.each do |pilot| %>
  <h2>Pilot Name:<%=pilot.name%></h2>
  <h2>Is A Captain:<%=pilot.is_captain%></h2>
  <h2>Years Of Experience:<%=pilot.years_experience%></h2>
  <%= link_to "Update Pilot",  "/airlines/#{@airline.id}/pilots/#{pilot.id}/edit"%> 
<% end %>
<%= link_to "Create Pilot", "/airlines/#{@airline.id}/pilots/new"%>